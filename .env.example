# Example environment file
# Port the server will bind to when running locally
PORT=3000

# SQLite database file path (for production/Docker, use /data/db.sqlite)
SQLITE_DB_PATH=./data/db.sqlite

# Web Push VAPID Keys
# Generate with: npx web-push generate-vapid-keys
VAPID_PUBLIC_KEY=
VAPID_PRIVATE_KEY=
NEXT_PUBLIC_VAPID_PUBLIC_KEY=
